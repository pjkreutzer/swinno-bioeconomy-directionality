@article{meyer2018QuantifyingResilienceRecurrent,
  title = {Quantifying Resilience to Recurrent Ecosystem Disturbances Using Flow\textendash Kick Dynamics},
  author = {Meyer, Katherine and {Hoyer-Leitzel}, Alanna and Iams, Sarah and Klasky, Ian and Lee, Victoria and Ligtenberg, Stephen and Bussmann, Erika and Zeeman, Mary Lou},
  year = {2018},
  month = nov,
  journal = {Nature Sustainability},
  volume = {1},
  number = {11},
  pages = {671--678},
  publisher = {{Nature Publishing Group}},
  issn = {2398-9629},
  doi = {10.1038/s41893-018-0168-z},
  abstract = {Shifting ecosystem disturbance patterns due to climate change (for example, storms, droughts and wildfires) or direct human interference (for example, harvests and nutrient loading) highlight the importance of quantifying and strengthening the resilience of desired ecological regimes. Although existing metrics capture resilience to isolated shocks, gradual parameter changes, and continual noise, quantifying resilience to repeated, discrete disturbance events requires different analytical tools. Here, we introduce a mathematical flow\textendash kick framework that uses dynamical systems tools to quantify resilience to disturbances explicitly in terms of their magnitude and frequency. We identify a boundary between disturbance regimes that cause either escape from, or stabilization within, a basin of attraction. We use the boundary to define resilience metrics tailored to repeated, discrete perturbations. The flow\textendash kick model suggests that the distance-to-threshold resilience metric overestimates resilience in the context of repeated perturbations. It also reveals counterintuitive triggers for regime shifts. These include increasing the periods between disturbance events in proportion to increases to disturbance magnitude, and\textemdash in systems with multiple dynamic variables\textemdash increasing time periods between disturbances of constant magnitude.},
  copyright = {2018 The Author(s), under exclusive licence to Springer Nature Limited},
  langid = {english},
  keywords = {Climate-change ecology,Climate-change impacts,Ecological modelling,Need to Read,Population dynamics,Theoretical ecology},
  file = {C\:\\Users\\ph8148kr\\Zotero\\storage\\8CLRBZDJ\\Meyer et al. - 2018 - Quantifying resilience to recurrent ecosystem dist.pdf}
}

@article{meyfroidt2010ForestTransitionsTrade,
  title = {Forest Transitions, Trade, and the Global Displacement of Land Use},

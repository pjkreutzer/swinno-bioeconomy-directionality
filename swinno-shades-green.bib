@article{hausknost2017TransitionWhichBioeconomy,
  title = {A {{Transition}} to {{Which Bioeconomy}}? {{An Exploration}} of {{Diverging Techno-Political Choices}}},
  shorttitle = {A {{Transition}} to {{Which Bioeconomy}}?},
  author = {Hausknost, Daniel and Schriefl, Ernst and Lauk, Christian and Kalt, Gerald},
  year = {2017},
  month = apr,
  journal = {Sustainability},
  volume = {9},
  number = {4},
  pages = {669},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  issn = {2071-1050},
  doi = {10.3390/su9040669},
  abstract = {To date the concept of the bioeconomy\textemdash an economy based primarily on biogenic instead of fossil resources\textemdash has largely been associated with visions of ``green growth'' and the advancement of biotechnology and has been framed from within an industrial perspective. However, there is no consensus as to what a bioeconomy should effectively look like, and what type of society it would sustain. In this paper, we identify different types of narratives constructed around this concept and carve out the techno-political implications they convey. We map these narratives on a two-dimensional option space, which allows for a rough classification of narratives and their related imaginaries into four paradigmatic quadrants. We draw the narratives from three different sources: (i) policy documents of national and supra-national authorities; (ii) stakeholder interviews; and (iii) scenarios built in a biophysical modelling exercise. Our analysis shows that there is a considerable gap between official policy papers and visions supported by stakeholders. At least in the case of Austria there is also a gap between the official strategies and the option space identified through biophysical modelling. These gaps testify to the highly political nature of the concept of the bioeconomy and the diverging visions of society arising from it.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {agro-ecology,bio-resources,bioeconomy,biotechnology,capitalism,sufficiency,technopolitics,transition},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\to-sort\\Hausknost et al. - 2017 - A Transition to Which Bioeconomy An Exploration o.pdf}
}

@article{hogbom2021TrilemmaNordicBaltic,
  title = {Trilemma of {{Nordic}}\textendash{{Baltic Forestry}}\textemdash{{How}} to {{Implement UN Sustainable Development Goals}}},

  author = {Mishra, Abhijeet and Humpen{\"o}der, Florian and Churkina, Galina and Reyer, Christopher P. O. and Beier, Felicitas and Bodirsky, Benjamin Leon and Schellnhuber, Hans Joachim and {Lotze-Campen}, Hermann and Popp, Alexander},

  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\phd-ideas\\swinno-research\\swinno-environmental-innovation\\possible-addtional-literature\\Kruse and Wetzel - Energy prices,technological knowledge and green en.pdf}

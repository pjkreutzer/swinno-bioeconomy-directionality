  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\SWINNO forest Chaminade\\Fischer et al. - 2020 - Swedish Forests in the Bioeconomy Stories from th.pdf}
}

@article{garcia-granero2018EcoinnovationMeasurementReview,
  title = {Eco-Innovation Measurement: {{A}} Review of Firm Performance Indicators},
  shorttitle = {Eco-Innovation Measurement},
  author = {{Garc{\'i}a-Granero}, Eva M. and {Piedra-Mu{\~n}oz}, Laura and {Galdeano-G{\'o}mez}, Emilio},

@article{bossle2016DriversAdoptionEcoinnovation,
  title = {The Drivers for Adoption of Eco-Innovation},
  author = {Bossle, Marilia Bonzanini and {Dutra de Barcellos}, Marcia and Vieira, Luciana Marques and Sauv{\'e}e, Lo{\"i}c},
  year = {2016},

@article{steffen2011AnthropoceneGlobalChange,
  title = {The {{Anthropocene}}: {{From Global Change}} to {{Planetary Stewardship}}},
  shorttitle = {The {{Anthropocene}}},
  author = {Steffen, Will and Persson, {\AA}sa and Deutsch, Lisa and Zalasiewicz, Jan and Williams, Mark and Richardson, Katherine and Crumley, Carole and Crutzen, Paul and Folke, Carl and Gordon, Line and Molina, Mario and Ramanathan, Veerabhadran and Rockstr{\"o}m, Johan and Scheffer, Marten and Schellnhuber, Hans Joachim and Svedin, Uno},
  year = {2011},

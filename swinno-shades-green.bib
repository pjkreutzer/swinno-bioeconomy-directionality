@article{sverdrup1998LongtermBaseCation,
  title = {Long-Term Base Cation Mass Balances for {{Swedish}} Forests and the Concept of Sustainability},
  author = {Sverdrup, H and Rosen, K},
  year = {1998},
  month = oct,
  journal = {FOREST ECOLOGY AND MANAGEMENT},

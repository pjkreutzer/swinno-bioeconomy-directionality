}

@article{garcia2018MethodologicalLuddismConcept,
  title = {Methodological {{Luddism}}: {{A}} Concept for Tying Degrowth to the Assessment and Regulation of Technologies},
  shorttitle = {Methodological {{Luddism}}},
  author = {Garcia, Jos{\'e} Lu{\'i}s and Jer{\'o}nimo, Helena Mateus and Carvalho, Tiago Mesquita},
  year = {2018},
  month = oct,
  journal = {Journal of Cleaner Production},
  volume = {197},

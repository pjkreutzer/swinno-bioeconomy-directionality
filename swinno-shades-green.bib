@article{brysse2013ClimateChangePrediction,
  title = {Climate Change Prediction: {{Erring}} on the Side of Least Drama?},
  shorttitle = {Climate Change Prediction},
  author = {Brysse, Keynyn and Oreskes, Naomi and O'Reilly, Jessica and Oppenheimer, Michael},
  year = {2013},

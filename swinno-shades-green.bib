@article{zolfagharian2021DynamicModelingTransition,
  title = {Toward the {{Dynamic Modeling}} of {{Transition Problems}}: {{The Case}} of {{Electric Mobility}}},
  shorttitle = {Toward the {{Dynamic Modeling}} of {{Transition Problems}}},

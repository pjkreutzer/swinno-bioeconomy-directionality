@article{giampietro2019CircularBioeconomyDecouplingb,
  title = {On the {{Circular Bioeconomy}} and {{Decoupling}}: {{Implications}} for {{Sustainable Growth}}},
  shorttitle = {On the {{Circular Bioeconomy}} and {{Decoupling}}},

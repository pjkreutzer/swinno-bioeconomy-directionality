  author = {Creutzig, Felix and Niamir, Leila and Bai, Xuemei and Callaghan, Max and Cullen, Jonathan and {D{\'i}az-Jos{\'e}}, Julio and Figueroa, Maria and Grubler, Arnulf and Lamb, William F. and Leip, Adrian and Masanet, Eric and Mata, {\'E}rika and Mattauch, Linus and Minx, Jan C. and Mirasgedis, Sebastian and Mulugetta, Yacob and Nugroho, Sudarmanto Budi and Pathak, Minal and Perkins, Patricia and Roy, Joyashree and {de la Rue du Can}, Stephane and Saheb, Yamina and Some, Shreya and Steg, Linda and Steinberger, Julia and {\"U}rge-Vorsatz, Diana},

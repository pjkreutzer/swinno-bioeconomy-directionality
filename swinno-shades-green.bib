  langid = {english},
  keywords = {Climate-change mitigation,Socioeconomic scenarios},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\climate-change\\Creutzig et al. - 2022 - Demand-side solutions to climate change mitigation.pdf}

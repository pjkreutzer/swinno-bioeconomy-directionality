@article{hurmekoski2019FrontiersForestbasedBioeconomy,
  title = {Frontiers of the Forest-Based Bioeconomy \textendash{} {{A European Delphi}} Study},
  author = {Hurmekoski, Elias and Lovri{\'c}, Marko and Lovri{\'c}, Nata{\v s}a and Hetem{\"a}ki, Lauri and Winkel, Georg},
  year = {2019},
  month = may,
  journal = {Forest Policy and Economics},
  volume = {102},
  pages = {86--99},
  issn = {1389-9341},
  doi = {10.1016/j.forpol.2019.03.008},
  abstract = {The European forest-based sector is undergoing major changes, while, at the same time, the role of a forest-based bioeconomy is being politically discussed. The aim of this study is to elicit expert views on critical changes in the sector through an outlook towards 2030 and 2050, as well as to analyze how these views relate to the understanding of bioeconomy. The study employs a three-round Delphi survey with 41 experts representing different sectors and societal perspectives across Europe. Forty statements based on initial explorative interviews were evaluated by the panel in two consecutive rounds of web-based questionnaires. Results reveal the highest consensus among panellists relates to the diversification of the forest industries and the dependence of the sector on climate and energy policies. The greatest dispersion of expert opinions was found to relate to the possible discontinuation of bioenergy market growth, the possible segregation of forest management, change in forest ownership structure, and the shortage of biomass supply in the long\textendash run. Additionally, the concept of bioeconomy was regarded by the majority as a vision for the future, rather than a depiction of real changes. Notably, major change regarding the significance of the sector, and specifically a transition towards a well-developed forest based bioeconomy, was not expected towards 2030 without significant change in the policy framework.},
  langid = {english},
  keywords = {Bioeconomy,Dissensus-based Delphi,Foresight,Forest-based sector,Trends,Uncertainties},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\SWINNO forest Chaminade\\Hurmekoski et al. - 2019 - Frontiers of the forest-based bioeconomy – A Europ.pdf}
}

@article{husa2021NonindustrialPrivateForest,
  title = {Non-Industrial Private Forest Owners' Willingness to Manage for Climate Change and Biodiversity},
  author = {Husa, Miikka and Kosenius, Anna-Kaisa},
  year = {2021},
  month = oct,
  journal = {Scandinavian Journal of Forest Research},
  volume = {0},
  number = {0},
  pages = {1--12},
  publisher = {{Taylor \& Francis}},
  issn = {0282-7581},
  doi = {10.1080/02827581.2021.1981433},
  abstract = {In boreal commercial forests, carbon sequestration, climate change adaptation, and biodiversity conservation can be promoted through various measures. This study examines the factors affecting non-industrial private forest (NIPF) owners' preferences for such forest management practices. A systematic literature review serves as a reference for the empirical analysis of a survey data on the Finnish NIPF owners' stated willingness to adopt thirteen distinct forest management practices. Binary logit models reveal socio-demographic factors, site-specific characteristics, previous forest management, and motivations for forest ownership that are associated with the stated adoption of management practices. Especially, environmental and financial motivations play an important role in decisions concerning forest management practices. Statistically significant factors vary depending on the forest management practice, reflecting the NIPF owner heterogeneity. Younger and highly educated forest owners are more supportive for various management practices that promote biodiversity, while older forest owners are reluctant towards deadwood retention. The results underline the importance of accounting for heterogeneous preferences regarding forest management practices when designing and implementing policies and advisory services aiming at enhancing carbon sequestration, climate change adaptation, or biodiversity in boreal commercial forests.},
  keywords = {biodiversity,carbon sequestration,climate change adaptation,commercial forests,forest management,Non-industrial private forest owners},
  annotation = {\_eprint: https://doi.org/10.1080/02827581.2021.1981433},
  file = {C\:\\Users\\ph8148kr\\Zotero\\storage\\F853ABMZ\\Husa and Kosenius - 2021 - Non-industrial private forest owners’ willingness .pdf}
}


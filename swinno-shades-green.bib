@article{wu2022DecouplingSDGsFollowed,
  title = {Decoupling of {{SDGs}} Followed by Re-Coupling as Sustainable Development Progresses},
  author = {Wu, Xutong and Fu, Bojie and Wang, Shuai and Song, Shuang and Li, Yingjie and Xu, Zhenci and Wei, Yongping and Liu, Jianguo},

@article{locatelli2022PeopleMindsGround,
  title = {In People's Minds and on the Ground: {{Values}} and Power in Climate Change Adaptation},
  shorttitle = {In People's Minds and on the Ground},
  author = {Locatelli, Bruno and Laurenceau, Martin and Chumpisuca, Yaneth Roxana Calla and Pramova, Emilia and Vallet, Am{\'e}line and Conde, Y{\'e}sica Quispe and Zavala, Ronal Cervantes and Djoudi, Houria and Lavorel, Sandra and Colloff, Matthew J.},
  year = {2022},

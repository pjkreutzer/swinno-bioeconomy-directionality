@article{holmgren2022WhoseTransformationThis,
  title = {Whose Transformation Is This? {{Unpacking}} the `Apparatus of Capture' in {{Sweden}}'s Bioeconomy},
  shorttitle = {Whose Transformation Is This?},
  author = {Holmgren, Sara and Giurca, Alexandru and Johansson, Johanna and Kanarp, Christoffer S{\"o}derlund and Stenius, Tove and Fischer, Klara},
  year = {2022},

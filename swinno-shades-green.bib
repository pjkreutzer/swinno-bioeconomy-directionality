@article{feiz2022KeyFactorsSiteselection,
  title = {Key Factors for Site-Selection of Biogas Plants in {{Sweden}}},
  author = {Feiz, Roozbeh and Metson, Genevi{\`e}ve S. and Wretman, Johanna and Ammenberg, Jonas},
  year = {2022},
  month = jun,
  journal = {Journal of Cleaner Production},
  volume = {354},
  pages = {131671},
  issn = {0959-6526},
  doi = {10.1016/j.jclepro.2022.131671},
  abstract = {Biogas production through anaerobic digestion is an integral part of the transition toward a biobased and circular economy and its expansion is foreseen in many parts of the world as well as in Europe. In Sweden, a governmental inquiry suggested biogas production to be increased from about 2~TWh today to 7~TWh by 2030. This rapid expansion would require installation of several new biogas plants across the country. However, the location of biogas plants can greatly affect its business performance and there are several geographic and socio-political factors that would limit the choice of location. Through dialogue with existing biogas producing companies and a few other related actors, we identified 12 factors that are commonly considered in the site-selection of biogas plants in Sweden or are considered to be important in the years to come. These factors are grouped into those related to supply and demand (feedstock supply, biogas demand, digestate demand, and carbon dioxide demand), infrastructure and synergies (available infrastructure, adjacent existing industries), land-use and zoning (nearby housing, zoning, and historic preservation sites), and socio-political context (political strategies and goals, organizational capability, and local social acceptance). We discuss how these factors can be used under rapidly transforming conditions in Sweden through different site-selection logics and highlight the importance of spatially explicit analysis for individual or coordinated decision making in future. Our method of enquiry and analysis, and to a certain degree the factors, can be also relevant for other countries, particularly in Europe. This study paves the way for more in-depth investigation of the question of site-selection of biogas plants in Sweden; both in the direction of detailed analysis at the local level, or screening analysis on the regional or national level for improved coordinated actions.},
  langid = {english},
  keywords = {Biogas,Facility location,Factors,Logistics,Qualitative,Spatial},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\phd-ideas\\Feiz et al. - 2022 - Key factors for site-selection of biogas plants in.pdf}
}

@article{feng2022DoublingAnnualForest,
  title = {Doubling of Annual Forest Carbon Loss over the Tropics during the Early Twenty-First Century},
  author = {Feng, Yu and Zeng, Zhenzhong and Searchinger, Timothy D. and Ziegler, Alan D. and Wu, Jie and Wang, Dashan and He, Xinyue and Elsen, Paul R. and Ciais, Philippe and Xu, Rongrong and Guo, Zhilin and Peng, Liqing and Tao, Yiheng and Spracklen, Dominick V. and Holden, Joseph and Liu, Xiaoping and Zheng, Yi and Xu, Peng and Chen, Ji and Jiang, Xin and Song, Xiao-Peng and Lakshmi, Venkataraman and Wood, Eric F. and Zheng, Chunmiao},
  year = {2022},
  month = feb,
  journal = {Nature Sustainability},
  pages = {1--8},
  publisher = {{Nature Publishing Group}},
  issn = {2398-9629},
  doi = {10.1038/s41893-022-00854-3},
  abstract = {Previous estimates of tropical forest carbon loss in the twenty-first century using satellite data typically focus on its magnitude, whereas regional loss trajectories and associated drivers are rarely reported. Here we used different high-resolution satellite datasets to show a doubling of gross tropical forest carbon loss worldwide from 0.97\,{$\pm$}\,0.16\,PgC\,yr-1 in 2001\textendash 2005 to 1.99\,{$\pm$}\,0.13\,PgC\,yr-1 in 2015\textendash 2019. This increase in carbon loss from forest conversion is higher than in bookkeeping models forced by land-use statistical data, which show no trend or a slight decline in land-use emissions in the early twenty-first century. Most (82\%) of the forest carbon loss is at some stages associated with large-scale commodity or small-scale agriculture activities, particularly in Africa and Southeast Asia. We find that \textasciitilde 70\% of former forest lands converted to agriculture in 2001\textendash 2019 remained so in 2020, confirming a dominant role of agriculture in long-term pan-tropical carbon reductions on formerly forested landscapes. The acceleration and high rate of forest carbon loss in the twenty-first century suggest that existing strategies to reduce forest loss are not successful; and this failure underscores the importance of monitoring deforestation trends following the new pledges made in Glasgow.},
  copyright = {2022 The Author(s)},
  langid = {english},
  keywords = {Carbon cycle,Sustainability,Tropical ecology},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\climate-change\\Feng et al. - 2022 - Doubling of annual forest carbon loss over the tro.pdf}
}


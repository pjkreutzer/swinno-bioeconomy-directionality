}

@article{javed2023ImpactGreenTechnologya,
  title = {The Impact of Green Technology Innovation, Environmental Taxes, and Renewable Energy Consumption on Ecological Footprint in {{Italy}}: {{Fresh}} Evidence from Novel Dynamic {{ARDL}} Simulations},

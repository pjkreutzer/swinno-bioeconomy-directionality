@article{hogbom2021TrilemmaNordicBaltic,
  title = {Trilemma of {{Nordic}}\textendash{{Baltic Forestry}}\textemdash{{How}} to {{Implement UN Sustainable Development Goals}}},
  author = {H{\"o}gbom, Lars and Abbas, Dalia and Armolaitis, K{\k{e}}stutis and Baders, Endijs and Futter, Martyn and Jansons, Aris and J{\~o}giste, Kalev and Lazdins, Andis and Lukmin{\.e}, Diana and Mustonen, Mika and {\O}istad, Knut and Poska, Anneli and Rautio, Pasi and Svensson, Johan and Vodde, Floor and {Varnagiryt{\.e}-Kaba{\v s}inskien{\.e}}, Iveta and Weslien, Jan and Wilhelmsson, Lars and Zute, Daiga},
  year = {2021},

  volume = {13},
  number = {10},
  pages = {5643},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/su13105643},
  abstract = {Forests are the dominant land cover in Nordic\textendash Baltic countries, and forestry, the management of forests for improved ecosystem-service (ES) delivery, is an important contributor to sustainability. Forests and forestry support multiple United Nations Sustainability Goals (UN SDGs) and a number of EU policies, and can address conflicting environmental goals. Forests provide multiple ecosystem services and natural solutions, including wood and fibre production, food, clear and clean water and air, animal and plant habitats, soil formation, aesthetics, and cultural and social services. Carbon sequestered by growing trees is a key factor in the envisaged transition from a fossil-based to a biobased economy. Here, we highlight the possibilities of forest-based solutions to mitigate current and emerging societal challenges. We discuss forestry effects on forest ecosystems, focusing on the optimisation of ES delivery and the fulfilment of UN SDGs while counteracting unwanted effects. In particular, we highlight the trilemma of (i) increasing wood production to substitute raw fossil materials, (ii) increasing forest carbon storage capacity, and (iii) improving forest biodiversity and other ES delivery.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  langid = {english},
  keywords = {ecosystem services,European Green Deal,forest,forestry,management practices,United Nations Sustainable Development Goals},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\courses\\SG0247-forest-sectors\\Högbom et al. - 2021 - Trilemma of Nordic–Baltic Forestry—How to Implemen.pdf}
}

@article{holmgren2022WhoseTransformationThis,
  title = {Whose Transformation Is This? {{Unpacking}} the `Apparatus of Capture' in {{Sweden}}'s Bioeconomy},
  shorttitle = {Whose Transformation Is This?},

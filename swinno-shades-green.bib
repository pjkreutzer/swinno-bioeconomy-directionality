@misc{arnold2022PraxisMakesPerfect,
  title = {Praxis Makes Perfect? {{The}} Lessons and Limitations of Past Experience for {{STI}} Policy in Transition},
  author = {Arnold, Erik and Serger, Sylvia Schwaag and Mackle, Luke},
  year = {2022},
  month = dec,
  publisher = {{OECD DIRECTORATE FOR SCIENCE, TECHNOLOGY AND INNOVATION COMMITTEE FOR SCIENTIFIC AND TECHNOLOGICAL POLICY}},
  abstract = {The digital and green transitions \textendash{} the process of economies and societies changing towards modes of living, organising, working, moving and producing that are digitally empowered, less carbon and emission intensive, and environmentally sustainable \textendash{} continue to have profound impacts on people, business and government. Owing to the significant role of science, technology and innovation (STI) in powering these transitions, they are also creating significant new demands for STI policy and governance. This paper sets out five areas where these transitions require a new approach, drawing from the literature on the current and previous transitions. To demonstrate the practical implications of the literature, the paper presents examples from Germany, drawing on the OECD Review of Innovation Policy: Germany (OECD, 2022[1]). We find that there is much relevant policy experience that can help STI meet contemporary transitional challenges, for example, on governance structures for STI to promote better co-ordination across ministries. However, we know much less on other important areas for STI to support current transitions, such as building agility, setting up experimentation and supporting breakthrough innovation.},
  langid = {english},
  lccn = {DSTI/STP/TIP(2022)26},
  file = {C\:\\Users\\ph8148kr\\Zotero\\storage\\A5DGQJDP\\Arnold et al. - Praxis makes perfect The lessons and limitations .pdf}
}

@article{arnould2022EmbeddingNonindustrialPrivate,
  title = {Embedding Non-Industrial Private Forest Owners in Forest Policy and Bioeconomy Issues Using a {{Living Lab}} Concept},
  author = {Arnould, Maxence and Morel, Laure and Fournier, Meriem},
  year = {2022},
  month = jun,
  journal = {Forest Policy and Economics},
  volume = {139},
  pages = {102716},
  issn = {1389-9341},
  doi = {10.1016/j.forpol.2022.102716},
  abstract = {International policies have set sustainable development goals that put emphasis on bioeconomy strategies based on renewable resources. The forestry sector, by providing bio-based products, is expected to take part in this bioeconomy with, among other things, the reduction of society's dependence on fossil fuels. Nevertheless, in Europe, the forestry sector is facing an increase in the number of small private forest ownerships, called non-industrial private forests (NIPF), where wood mobilization is difficult due to both ownership fragmentation and the lack of interest of non-industrial forest owners in existing forest-wood chains. Although many policy instruments have been put in place to address this situation, the problem persists for two main reasons. First, a lack of use of policy instruments by forest owners and second, a lack of collaboration between stakeholders. To provide solutions, we propose a methodology to design territorial projects with non-industrial forest owners in the framework of a Living Lab innovation process. This paper presents both the general method developed and analyzed through open and user-centered innovation concepts and its practical implementation in the Vosges department in France. Our results show how the Living Lab approach can improve the acceptance, adoption and use of policy instruments by NIPF owners and how it promotes multi-stakeholder collaborations to design and deploy innovative solutions. The main interest of our study is to provide a methodology to pilot a forestry Living Lab for policy makers and practitioners, based on rigorous concepts of innovation management. Finally, future developments and limitations of our study are discussed in a global research perspective.},
  langid = {english},
  keywords = {Action research,French forest policy,Living Lab,Wood mobilization}
}


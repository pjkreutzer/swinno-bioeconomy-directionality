@article{jonsson2022ContinuityChangeForest,
  title = {Continuity and Change in Forest Restoration. {{A}} Comparison of {{US}} Ecology and Forestry in the 1940s and 1990s},
  author = {J{\"o}nsson, Jimmy and Priebe, Janina and M{\aa}rald, Erland and Lundmark, Tomas},

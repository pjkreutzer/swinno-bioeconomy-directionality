  keywords = {Energy efficiency,Knowledge spillovers,Network analysis,O33,O38,Q48,Q55,Residential sector,Technological capability},
  file = {C\:\\Users\\ph8148kr\\OneDrive - Lund University\\literature\\phd-ideas\\swinno-research\\swinno-environmental-innovation\\possible-addtional-literature\\Costantini et al. - 2023 - Network-driven positive externalities in clean ene.pdf}
